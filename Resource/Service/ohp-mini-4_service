[Unit]
Description=OHP Mini ( Universal )
Documentation=https://github.com/sunbear88/scvps
After=network.target nss-lookup.target

[Service]
User=root
NoNewPrivileges=true
ExecStart=/usr/local/sunbearvpn/ohp-mini -port 8091 -proxy 127.0.0.1:3128 -tunnel 0.0.0.0:0
Restart=on-failure

[Install]
WantedBy=multi-user.target
